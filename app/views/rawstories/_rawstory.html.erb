<% div_for(rawstory) do %>
 <b> <%= link_to rawstory.title, rawstory.link %><%= ' (opinion)' if rawstory.opinion == 1 %></b>
<br>

<%= link_to rawstory.author.name, rawstory.author %><%= ' - ' if rawstory.author.name != '' %><%= link_to rawstory.source.name, rawstory.source  %> - <%= rawstory.created_at %> <br><br>

<% end %>